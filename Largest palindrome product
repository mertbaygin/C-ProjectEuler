#include <stdio.h>
#include <stdlib.h>
/* Sayıyı tersden yazdıran fonksiyon*/
int donustur(int x)
{
    int kalan=0,sonuc=0;
    while(x>0)
    {
        kalan=x%10;
        sonuc=sonuc*10+kalan;
        x/=10;
    }
    return sonuc;
}

int main()
{
    int j=999,i=999,k;
    {

        while(i>0)
        {
            k=i*j;
            while(k==donustur(k))
            {
                printf("%d Max Digit number :",k);
                printf(" i:%d j:%d k: %d\n",i,j,k);
                return 0;
            }
            i=i-1;
        }

    }



}
