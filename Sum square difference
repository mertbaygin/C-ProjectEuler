/* a= 1 ile 100 arasındaki sayıların herbirinin karesinin toplamıyla*/
/* b=1 ile 100 arasındaki satıyılarının tamammımın karesi*/
/*c =b-a */

#include <stdio.h>
#include <stdlib.h>

int main()
{
    int sum=0,square=0,result=0;
    for(int i=1; i<=100; i++)
    {
        square+=i*i;
        sum+=i;
    }
    result=(sum*sum) - square;
    printf("Square:%d Sum:%d\n",square,sum);
    printf("%d",result);
    return 0;
}
